
@using BookApp.BLL.Services.Tags
@model IEnumerable<TagListItemDto>
@{
    ViewBag.Title = "Tags";
}

<h2>Tags</h2>
<br />
<table class="table">
    <tr>
        <th>tag</th>
    </tr>
    @foreach (var tag in Model)
    {
        <tr>
            <td>@tag.Text</td>
        </tr>
    }
</table>
<hr />
<br />
<div class="text-center">
    <a asp-action="Create"
       asp-controller="Tag"
       asp-area="contentmanager"
       class="btn btn-primary">create</a>
</div>